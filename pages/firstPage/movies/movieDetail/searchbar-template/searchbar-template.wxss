.searchbar {
  width: 90%;
  height: 50rpx;
  display: flex;
  flex-direction: row;
  margin: 5%;
  border: 1px solid black;
  border-radius: 10rpx;
}

.icon {
  vertical-align: middle;
  margin: auto 0px auto 5px;
  margin-right: 5px;
}

.inputtext {
  height: 50rpx;
  width: 85%;
  margin-right: 5px;
}

.placeholder {
  font-size: 14px;
}

.cancel {
  vertical-align: middle;
  margin: auto 5px auto 0px;
}
